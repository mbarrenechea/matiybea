<p id="notice"><%= notice %></p>

<h1>Listing Enquires</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enquires.each do |enquire| %>
      <tr>
        <td><%= enquire.name %></td>
        <td><%= enquire.score %></td>
        <td><%= link_to 'Show', enquire %></td>
        <td><%= link_to 'Edit', edit_enquire_path(enquire) %></td>
        <td><%= link_to 'Destroy', enquire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enquire', new_enquire_path %>
