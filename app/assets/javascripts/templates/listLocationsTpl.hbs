<div class="m-location-categories">
  <h3>Selecciona tu plan</h3>
  <ul>
    {{#each this.categories}}
      <li class="{{this.category}}" data-category="{{this.category}}">
        <div class="field -checkbox">
          <input type="checkbox" checked="checked" id="location-category-{{this.category}}">
          <label for="location-category-{{this.category}}">{{this.category_name}}</label>            
        </div>
      </li>
    {{/each}}
  </ul>
</div>
<div class="m-location-list">
  <h3>Listado de planes</h3>
  <ul>
    {{#each this.locations}}
      <li>
        <div class="-img {{this.category}}"></div>
        <div class="-content">
          <h4>{{this.category_name}}</h4>
          <h3>{{this.name}}</h3>
        </div>
      </li>
    {{/each}}    
  </ul>
</div>